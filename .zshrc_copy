function nvimcommit(){

  if [ ! -z "$1" ]; then
    CURDIR=$PWD
    cd ~/.config/nvim
    git checkout master
    # cp ~/.zshrc ./.zshrc_copy
    # cp ~/.vimrc ./.vimrc_copy
    git add .
    git commit -m "$1"
    git push -u origin master
    cd "$CURDIR"
    unset CURDIR
  fi

}
